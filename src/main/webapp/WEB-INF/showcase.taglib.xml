<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib id="demo"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd"
    version="2.2"
>
    <namespace>http://omnifaces.org/showcase</namespace>
    <composite-library-name>showcase</composite-library-name>

	<!-- Demo tags, used in methodParam page only. -->
	<tag>
  		<tag-name>actionmethod</tag-name>
  		<source>tags/actionmethod.xhtml</source>
	</tag>
	<tag>
  		<tag-name>actionlistener</tag-name>
  		<source>tags/actionlistener.xhtml</source>
	</tag>
	<tag>
  		<tag-name>actionlistenertrick</tag-name>
  		<source>tags/actionlistenertrick.xhtml</source>
	</tag>
	<tag>
  		<tag-name>globalSearch</tag-name>
  		<source>tags/globalSearch.xhtml</source>
	</tag>
	<tag>
  		<tag-name>githubLink</tag-name>
  		<source>tags/githubLink.xhtml</source>
  		<attribute>
  			<name>issue</name>
  			<type>java.lang.Integer</type>
  		</attribute>
  		<attribute>
  			<name>commit</name>
  			<type>java.lang.String</type>
  		</attribute>
	</tag>
	
</facelet-taglib>